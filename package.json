{
  "name": "stellar-wallet",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "SatoshiPay Ltd",
  "repository": "github:satoshipay/wallet",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rm -rf dist/",
    "build": "NODE_ENV=production parcel build --public-url=./ --detailed-report src/index.html",
    "dev": "NODE_ENV=development parcel src/index.html -p 3000",
    "lint:fix": "tslint --fix '{src,stories}/**/*.{ts,tsx}'",
    "prettier": "prettier --write '{src,stories}/**/*.{ts,tsx}' *.md electron/*.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "tslint --project ."
  },
  "lint-staged": {
    "ignore": [
      "**/*.yml"
    ],
    "linters": {
      "*": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@storybook/addon-centered": "^3.4.8",
    "@storybook/react": "^3.3.14",
    "@types/big.js": "^4.0.5",
    "@types/history": "^4.6.2",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-icons": "^2.2.5",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/recompose": "^0.26.1",
    "@types/stellar-sdk": "^0.8.3",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^3.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-typescript": "^0.7.1",
    "prettier": "^1.13.7",
    "standard": "^10.0.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^2.0.0",
    "big.js": "^5.0.3",
    "history": "^4.7.2",
    "key-store": "^1.0.0-beta.2",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-human-time": "^1.1.0",
    "react-icons": "^2.2.7",
    "react-promise": "^2.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "stellar-sdk": "^0.8.2"
  }
}
