{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es2017",
    "resolveJsonModule": true,
    "lib": ["es6", "dom"],
    "jsx": "react",
    "noUnusedLocals": true,
    "skipLibCheck": true
  }
}
